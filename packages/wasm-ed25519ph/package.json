{
  "author": "fran <fran.sl@atlascity.io>",
  "contributors": [
    "nshcore <nshcore@protonmail.com>",
    "fran <fran.sl@atlascity.io>",
    "Richard Littauer <richard+github@burntfen.com>"
  ],
  "description": "WASM build of Rust bulletproof cryptography",
  "bugs": "https://github.com/catalyst-network/wasm-ed25519ph/issues",
  "homepage": "https://github.com/catalyst-network/wasm-ed25519ph/",
  "repository": "catalyst-network/wasm-ed25519ph",
  "keywords": [
    "wasm",
    "rust",
    "cryptography",
    "crypto",
    "catalyst"
  ],
  "license": "MIT",
  "name": "wasm-ed25519",
  "version": "0.1.0",
  "scripts": {
    "nuke:fosho": "rimrafdist pkg node_modules submodules package-lock.json yarn.lock",
    "build": "rimraf dist pkg && webpack -p --progress --config build/webpack.prod.babel.js --fix",
    "start": "rimraf dist pkg && webpack-dev-server --open -d --progress --color --config build/webpack.dev.babel.js",
    "test:cargo": "cargo test && wasm-pack test --headless",
    "lint:js": "eslint --ignore-path .gitignore pkg/**/*.js --fix",
    "test": "jest tests  --detectOpenHandles --debug",
    "test:watch": "npm jest pkg --watch --notify",
    "lint": "eslint --cache --color --ext .jsx,.js tests --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm lint:fix"
    }
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "regenerator-runtime": "^0.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-async-to-generator": "^7.7.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@wasm-tool/wasm-pack-plugin": "1.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-rewire": "^1.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "url-loader": "^2.0.0",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
