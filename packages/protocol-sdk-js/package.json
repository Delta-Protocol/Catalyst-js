{
  "name": "protocol-sdk-js",
  "version": "0.0.0",
  "description": "Catalyst Protocol Js SDK",
  "main": "src/index.js",
  "scripts": {
    "test": "test:unit",
    "build": "protoc --plugin=\"protoc-gen-ts=\"./node_modules/.bin/protoc-gen-ts\"\" --proto_path Protocol/src/ --js_out=import_style=commonjs,binary:src --ts_out=\"src\" Protocol/src/*.proto"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catalyst-network/protocol-sdk-js.git"
  },
  "keywords": [
    "catalyst-network",
    "blockchain",
    "dlt"
  ],
  "author": "James Kirby <nshcore@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/catalyst-network/protocol-sdk-js/issues"
  },
  "homepage": "https://github.com/catalyst-network/protocol-sdk-js#readme",
  "devDependencies": {
    "google-protobuf": "^3.11.2",
    "protobufjs": "^6.8.8",
    "ts-protoc-gen": "^0.12.0"
  },
  "dependencies": {
    "@types/google-protobuf": "^3.7.2"
  }
}
